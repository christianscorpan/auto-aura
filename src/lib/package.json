{
  "name": "dmr-scraper",
  "version": "1.0.0",
  "description": "A TypeScript scraper for the Danish Motor Register.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "example": "ts-node src/example.ts",
    "test": "ts-node src/test.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "dmr",
    "scraper",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "axios-cookiejar-support": "^5.0.2",
    "cheerio": "^1.0.0-rc.12",
    "tough-cookie": "^4.1.4"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/tough-cookie": "^4.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
